function realpath() {
	python -c "import os,sys; print os.path.realpath(sys.argv[1])" $0
}

CONTENTS="$(dirname "$(dirname "$(realpath "$0")")")"
ELECTRON="$ROOT/MacOS/Electron"
BOOTSTRAP="$ROOT/Resources/bootstrap.js"

echo $ELECTRON
echo $BOOTSTRAP